<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4a3949"
    android:clickable="true"
    android:gravity="center"
    android:orientation="vertical">

    <!--<ImageView
        android:id="@+id/imgIcon"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity=""
        android:src="@drawable/photo" />-->

    <include android:id="@+id/top"/>

    <TextView
        android:id="@+id/feed_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/feed_xx"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:text="FEED"
        android:textColor="#ffff"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/explore_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/explore_xx"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:layout_marginTop="20dp"
        android:text="EXPLORE"
        android:textColor="#ffff"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/cuisines_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/cuisines_xx"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:text="CUISINES"
        android:textColor="#ffff"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/messages_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/messages_xx"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:text="MESSAGES"
        android:textColor="#ffff"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/followers_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/followers_xxx"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:text="FOLLOWERS"
        android:textColor="#ffff"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/signout_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:drawableLeft="@drawable/signout"
        android:layout_marginStart="50dp"
        android:drawablePadding="30dp"
        android:text="SIGN OUT"
        android:textColor="#ffff"
        android:textSize="20dp"/>


</LinearLayout>